{
    "version": 2,
    "name": "pdf-viewer",
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "headers": {
          "X-Frame-Options": "ALLOWALL",
          "Content-Security-Policy": "frame-ancestors *;"
        }
      }
    ]
  }
  