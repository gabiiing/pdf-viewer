{
    "version": 2,
    "name": "pdf-viewer",
    "builds": [
      { "src": "index.html", "use": "@vercel/static" },
      { "src": "middleware.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "headers": {
          "X-Frame-Options": "ALLOWALL",
          "Content-Security-Policy": "frame-ancestors *;"
        }
      }
    ]
  }
  